{
    "name": "destinations-api",
    "routes": {
        "destination": {
            "POST": [{
                "route": "/api/v1/destinations/createDestination",
                "description": "creates a new destination"
            }],
            "PUT": [{
                "route": "/api/v1/destinations/updateDestination/:destinationID",
                "description": "updates an existing destination using destinationID"
            },
            {
                "route": "/api/v1/destinations/updateDestination/:destinationName",
                "description": "updates an existing destination using destinationName"
            }]
        }
    }

}