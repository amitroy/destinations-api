{
  "extends": "airbnb",
  "rules": {
    "no-console": 0,
    "eol-last": 0,
    "indent": "off",
    "comma-dangle": "off",
    "prefer-destructuring": "off",
    "react/prefer-stateless-function": "off",
    "react/jsx-filename-extension": [
      "on",
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/jsx-one-expression-per-line": "off"
  }
}
